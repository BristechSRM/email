<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
  </startup>
  <appSettings file="Creds.config">
    <!--If you haven't created a Credentials config, go to Credentials.fs for details. EmailAccount and Password must be set-->
    <add key="EmailAccount" value=""/>
    <add key="Password" value =""/>

    <add key="CommsUrl" value="http://comms:8080"/>
    <add key="SessionsUrl" value="http://sessions:8080"/>
    
    <add key="PollingInterval" value="00:01:00"/>
    
    <add key="serilog:minimum-level" value="Debug"/>
    <add key="serilog:write-to:ColoredConsole"/>
  </appSettings>
</configuration>